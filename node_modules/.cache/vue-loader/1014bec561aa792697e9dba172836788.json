-icon>\n\n          <v-list-item-content>\n            <!-- <router-link >{{dir}}</router-link> -->\n            <v-list-item-title\n              @click=\"$route.path !== `${dir}` ? $router.push(dir) : null\"\n              >{{ text }}</v-list-item-title\n            >\n          </v-list-item-content>\n        </v-list-item>\n      </v-list>\n    </v-navigation-drawer>\n\n    <v-main>\n      <v-container class=\"py-8 px-6\" fluid>\n        <v-row>\n          <router-view />\n        </v-row>\n      </v-container>\n    </v-main>\n  </v-app>\n</template>\n\n<script>\nimport axios from 'axios'\n\nexport default {\n  name: \"App\",\n\n  components: {\n    // HelloWorld,\n  },\n\n  data: () => ({\n    cards: [\"Today\", \"Yesterday\"],\n    drawer: false,\n        links: [\n      [\"mdi-inbox-arrow-down\", \"Inbox\", \"/\"],\n      [\"mdi-send\", \"Send\", \"/About\"],\n      [\"mdi-delete\", \"Trash\", \"/NotFound\"],\n      [\"mdi-alert-octagon\", \"Spam\", \"/\"],\n    ],\n  }),\n  methods: {\n    theme() {\n      this.$vuetify.theme.dark = !this.$vuetify.theme.dark;\n      console.log(this.$vuetify);\n      console.log(this.$route.path);\n    },\n    getProducts(){\n       \n      var url = \"https://talesserve.herokuapp.com/tales\";\n\n      axios\n        .get(url)\n        .then(res => {\n          this.tales = res.data.found;\n          this.tales = this.tales.reverse();\n        })\n        .then(() => {\n          this.activestory = this.tales[0];\n        });\n    }\n  },\n    }\n  },\n};\n</script>\n"]}]}